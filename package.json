    "axios": "^0.25.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.3",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "styled-components": "^5.3.3"
  }
}
